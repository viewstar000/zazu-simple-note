{
  "name": "zazu-note",
  "version": "1.0.0",
  "description": "Simple Note manager based zazu.",
  "blocks": {
    "input": [
      {
        "id": "NoteIndexer",
        "type": "PrefixScript",
        "prefix": "note",
        "space": true,
        "args": "Optional",
        "script": "src/note_search.js",
        "connections": [
          "OpenNote"
        ]
      },
      {
        "id": "NoteIndexerRefresh",
        "type": "Keyword",
        "keyword": "noterefresh",
        "title": "Rebuild Note Index",
        "connections": [
          "RefreshNote"
        ]
      }
    ],
    "output": [
      {
        "id": "OpenNote",
        "type": "OpenFile"
      },
      {
        "id": "RefreshNote",
        "type": "OpenInBrowser",
        "url": "http://localhost:24306/refresh_index"
      }
    ]
  }
}