{
  "name": "zazu-note",
  "version": "1.0.0",
  "description": "Simple Note manager based zazu.",
  "stylesheet": "src/main.css",
  "blocks": {
    "input": [{
      "id": "NoteIndexer",
      "type": "PrefixScript",
      "prefix": "note",
      "space": true,
      "args": "Optional",
      "script": "src/note.js",
      "connections": ["OpenNote"]
    }],
    "output": [{
      "id": "OpenNote",
      "type": "OpenFile"
    }]
  }
}